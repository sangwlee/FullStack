<script type="text/javascript">
  <% if logged_in? %>
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  <% end %>

  window.staticImages = {
    slicklogo: "<%= asset_path('slicklogo.png') %>",
    background: "<%= asset_path('background.png') %>"
  }

</script>
<main id="root"></main>
